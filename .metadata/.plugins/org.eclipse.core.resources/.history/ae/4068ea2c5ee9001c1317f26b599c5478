#include <stdint.h>
#include "gpio.h"
#include "system_clock.h"
#include"interrupt_PA0.h"

int interrupt_PA0 =0;
int main(void)
{
    config();
	init_GP(PC,13,OUT50,O_GP_PP);
	init_GP(PA,0,IN,I_PP);
	W_GP(PC, 13, LOW);
	init_interrupt_PA0();




	while(1){
		if(interrupt_PA0){
			 delay_ms(1000);
			toggle_GP(PC, 13);
				delay_ms(1000);
		}else{
			W_GP(PC, 13, HIGH);
		}
	}
}

